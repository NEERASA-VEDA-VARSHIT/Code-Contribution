public class Solution {
    public int[][] solve(int[][] A) {
        int [][] C = new int[A[0].length][A.length];

        for (int i = 0; i < A.length; i++) {
            for (int j = 0; j < A[0].length; j++) { 
                C[j][i] = A[i][j];              
            }
        }

        return C;
        
    }
}
